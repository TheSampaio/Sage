// math.sg

module math
{
    func sum(x: f32, y: f32): f32
    {
        return x + y;
    }

    func subtract(x: f32, y: f32): f32
    {
        return x - y;
    }

    func multiply(x: f32, y: f32): f32
    {
        return x * y;
    }

    func divide(x: f32, y: f32): f32
    {
        return x / y;
    }
}
